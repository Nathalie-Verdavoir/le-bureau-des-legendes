{{ form_start(form) }} <button class="btn btn-success inlineForm">{{ button_label|default('Enregistrer') }}</button>
 <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>
      {{ form_row(form.agent) }}
  

   {{ form_label(form.specialites) }} 
        <div class="list">
            {% for field in form.specialites %}
                <div class="checkbox-list">
                    {{ form_widget(field) }}
                    {{ form_label(field) }}
                </div>
            {% endfor %}
        </div>  
{{ form_end(form) }}
