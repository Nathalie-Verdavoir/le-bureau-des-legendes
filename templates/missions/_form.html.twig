{{ form_start(form) }}
    <button class="btn btn-success inlineForm">{{ button_label|default('Enregistrer') }}</button>
      {{ form_row(form.titre) }}
    {{ form_row(form.description) }}
    {{ form_row(form.date_debut) }}
    
    {{ form_row(form.date_fin) }}
    {{ form_row(form.nom_de_code) }}
    {{ form_row(form.type) }}
    {{ form_row(form.statut) }}
    {{ form_row(form.specialite) }}
    {{ form_row(form.pays) }}

    {{ form_label(form.planques) }} 
        <div class="list">
            {% for field in form.planques %}
                <div class="checkbox-list">
                    {{ form_widget(field) }}
                    {{ form_label(field) }}
                </div>
            {% endfor %}
        </div>
    {{ form_label(form.agents) }} 
        <div class="list long">
            {% for field in form.agents %}
                <div class="checkbox-list">
                    {{ form_widget(field) }}
                    {{ form_label(field) }}
                </div>
            {% endfor %}
        </div>
    {{ form_label(form.contacts) }} 
        <div class="list">
            {% for field in form.contacts %}
                <div class="checkbox-list">
                    {{ form_widget(field) }}
                    {{ form_label(field) }}
                </div>
            {% endfor %}
        </div>
    {{ form_label(form.Cibles) }} 
        <div class="list">
            {% for field in form.Cibles %}
                <div class="checkbox-list">
                    {{ form_widget(field) }}
                    {{ form_label(field) }}
                </div>
            {% endfor %}
        </div>
    
{{ form_end(form) }}